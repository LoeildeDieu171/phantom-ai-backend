{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
{
  "functions": {
    "api/chat.js": {
      "runtime": "edge"
    }
  }
}
